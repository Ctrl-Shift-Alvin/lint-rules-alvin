      "optional": true
    },
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-jsonc": {
      "optional": true
    },
    "eslint-plugin-markdown": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
