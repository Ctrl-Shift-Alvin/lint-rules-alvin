  "keywords": [
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ctrl-Shift-Alvin/lint-rules-alvin"
  },
  "license": "MIT",
  "author": "Alvin Raul Sz≈ëke",
  "type": "module",
  "exports": {
    "./eslint": {
