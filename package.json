      "types": "./eslint/presets/index.d.ts"
    },
    "./eslint/configs": "./eslint/configs/index.js",
    "./eslint/configs/astro": "./eslint/configs/astro.js",
    "./eslint/configs/astro_ts": "./eslint/configs/astro_ts.js",
    "./eslint/configs/base": "./eslint/configs/base.js",
    "./eslint/configs/import_x": "./eslint/configs/import_x.js",
    "./eslint/configs/import_x_ts": "./eslint/configs/import_x_ts.js",
    "./eslint/configs/json": "./eslint/configs/json.js",
    "./eslint/configs/markdown": "./eslint/configs/markdown.js",
    "./eslint/configs/react_hooks": "./eslint/configs/react_hooks.js",
    "./eslint/configs/stylistic": "./eslint/configs/stylistic.js",
    "./eslint/configs/typescript": "./eslint/configs/typescript.js"
  },
  "peerDependencies": {
    "eslint": "^9.37.0",
    "@eslint/compat": "^1.4.0",
